<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane prefHeight="900.0" prefWidth="1140.0" stylesheets="@css/settings.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.rms.rms.SettingsController">

   <children>
      <HBox alignment="CENTER" StackPane.alignment="CENTER">
         <children>
            <VBox prefHeight="840.0" prefWidth="398.0" spacing="54.0" style="-fx-background-color: #d9d9d9;">
               <children>
                  <Button fx:id="addMenuItems" mnemonicParsing="false" onAction="#handleAddMenuItemsClick" prefHeight="65.0" prefWidth="338.0" styleClass="settingButtons" text="Add Menu Items">
                     <font>
                        <Font name="Arial Bold" size="18.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" prefHeight="65.0" prefWidth="338.0" styleClass="settingButtons" text="Account Settings">
                     <font>
                        <Font name="Arial Bold" size="18.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" prefHeight="65.0" prefWidth="338.0" styleClass="settingButtons" text="Button" />
                  <Button mnemonicParsing="false" prefHeight="65.0" prefWidth="338.0" styleClass="settingButtons" text="Button" />
               </children>
               <padding>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </padding>
            </VBox>
         </children>
         <padding>
            <Insets bottom="30.0" top="30.0" />
         </padding>
      </HBox>
   </children>
</StackPane>
